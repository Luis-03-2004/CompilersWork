procedimento inteiro fatorial(inteiro n)
inicio
   inteiro r;
   se n < 0 entao
      retorna 0;
   fimse

      r <- fatorial(n-1);
      retorna n * r;
fim

procedimento inteiro soma(inteiro n)
inicio
   inteiro r;
   se n < 0 entao
      retorna 0;
   fimse

   se n = 0 ou n = 1 entao
      retorna 1;
   senao
      retorna fatorial(n);
      r <- fatorial(n-1);
      retorna n * r;
   fimse
fim

procedimento vazio imprime(inteiro n)
inicio
   escreva n;
fim

